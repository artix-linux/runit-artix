#!/bin/bash

. @RCLIBDIR@/functions

print_leave

# [[ -x @RCDIR@/rc.local.shutdown ]] && @RCDIR@/rc.local.shutdown

print_leave

RC_SHUTDOWN=(random-seed timezone cleanup udev kill-all swap api-fs mount-ro)

for service in ${RC_SHUTDOWN[@]};do
    case $service in
        kill-all|mount-ro) rc-sv start $service ;;
        *) rc-sv stop $service ;;
    esac
done
